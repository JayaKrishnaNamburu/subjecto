{
  "name": "subjecto",
  "version": "0.0.31",
  "description": "A minimalistic state management library",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": "(/src/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/subject.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "github.com/paulbrie/subjecto"
  },
  "keywords": [
    "state",
    "management",
    "subscribe",
    "data",
    "store"
  ],
  "author": "Paul Brie",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  }
}
